{
  "name": "xtypescript",
  "version": "1.0.0",
  "description": "Exercism exercises in Typescript.",
  "author": "",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/exercism/xtypescript"
  },
  "scripts": {
    "test": "jest --watchAll",
    "lint": "tslint \"*.ts?(x)\""
  },
  "dependencies": {
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.2.4"
  }
}
